{
  "name": "octelium-portal",
  "private": true,
  "version": "0.0.0-main",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "build-dev": "tsc && vite build --mode development",
    "build-staging": "tsc && vite build --mode staging",
    "protoc-gen-auth": "npx protoc --ts_out ./tmp --ts_opt long_type_number --proto_path ../../../../../../pb apis/protobuf/main/authv1/authv1.proto",
    "protoc-gen-user": "npx protoc --ts_out ./tmp --ts_opt long_type_number --proto_path ../../../../../../pb apis/protobuf/main/userv1/userv1.proto",
    "protoc-gen-meta": "npx protoc --ts_out ./tmp --ts_opt long_type_number --proto_path ../../../../../../pb apis/protobuf/main/metav1/metav1.proto",
    "protoc-gen": "mkdir -p ./tmp/apis/protobuf && npm run protoc-gen-meta && npm run protoc-gen-user && npm run protoc-gen-auth",
    "protoc-cp": "cp -r ./tmp/apis/protobuf/main/metav1 ./src/apis && cp -r ./tmp/apis/protobuf/main/userv1 ./src/apis && cp -r ./tmp/apis/protobuf/main/authv1 ./src/apis && find ./src -name \"*.proto\" -type f -delete",
    "protoc-fix": "find ./src/apis -name '*.ts' -exec sed -i s#../../../../google/protobuf#../google/protobuf#g {} +",
    "protoc": "npm run protoc-gen && npm run protoc-cp && npm run protoc-fix"
  },
  "dependencies": {
    "@mantine/core": "^8.3.13",
    "@mantine/hooks": "^8.3.13",
    "@protobuf-ts/grpcweb-transport": "^2.11.1",
    "@protobuf-ts/plugin": "^2.11.1",
    "@reduxjs/toolkit": "^2.11.2",
    "@tailwindcss/postcss": "^4.1.18",
    "@tanstack/react-query": "^5.90.19",
    "dayjs": "^1.11.19",
    "framer-motion": "^12.28.1",
    "lucide-react": "^0.562.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-icons": "^5.5.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.12.0",
    "react-spinners": "^0.17.0",
    "tailwind-merge": "^3.4.0",
    "ts-pattern": "^5.9.0",
    "vite-plugin-svgr": "^4.5.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^29.0.0",
    "@types/node": "^25.0.10",
    "@types/react": "^19.2.9",
    "@types/react-copy-to-clipboard": "^5.0.7",
    "@types/react-dom": "^19.2.3",
    "@types/truncate-utf8-bytes": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.39.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "postcss": "^8.5.6",
    "rollup-plugin-visualizer": "^6.0.5",
    "sass": "^1.97.3",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  }
}
